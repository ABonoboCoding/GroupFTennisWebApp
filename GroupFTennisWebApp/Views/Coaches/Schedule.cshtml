@model IEnumerable<GroupFTennisWebApp.Models.ScheduleMembers>

@{
    ViewData["Title"] = "MySchedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Member Email</h2>

@foreach (var items in Model)
{
  <p>@Html.DisplayFor(modelItem => items.MemberEmail)</p>

}
<div>

    @if (User.IsInRole("Admin"))
    {
        <a asp-action="AllSchedule">Back to Schedule</a>
    }

    @if (User.IsInRole("Coach"))
    {
        <a asp-action="MySchedule">Back to Schedule</a>
    }

</div>